{
	"name": "angular-technical-test",
	"version": "1.0.0",
	"license": "MIT",
	"description": "Technical test for Frontend Engineer position at Ictechlab",
	"scripts": {
		"ng": "ng",
		"start": "npm run start:all",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"lint": "ng lint",
		"postinstall": "cd api && npm ci",
		"format:check": "prettier --check \"src/**/*.{ts,html,scss,json}\"",
		"format:write": "prettier --write \"src/**/*.{ts,html,scss,json}\"",
		"start:all": "concurrently \"cd api && npm start\" \"ng serve --configuration development\""
	},
	"private": true,
	"dependencies": {
		"@angular/cdk": "19.2",
		"@angular/common": "^19.2.0",
		"@angular/compiler": "^19.2.0",
		"@angular/core": "^19.2.0",
		"@angular/forms": "^19.2.0",
		"@angular/platform-browser": "^19.2.0",
		"@angular/platform-browser-dynamic": "^19.2.0",
		"@angular/router": "^19.2.0",
		"bootstrap": "^5.3.8",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.15.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^19.2.15",
		"@angular/cli": "^19.2.15",
		"@angular/compiler-cli": "^19.2.0",
		"@types/jasmine": "~5.1.0",
		"@types/node": "^24.5.2",
		"angular-eslint": "20.2.0",
		"concurrently": "^9.2.1",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"jasmine-core": "~5.6.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"prettier": "^3.6.2",
		"typescript": "~5.7.2",
		"typescript-eslint": "8.40.0"
	}
}
